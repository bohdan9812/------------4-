{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/flow",
      "destination": "/flow/index.html"
    },
    {
      "source": "/flow/(.*)",
      "destination": "/flow/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/flow",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/flow/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
